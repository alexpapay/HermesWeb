        </tr>
        <tr>
            <td>@Html.DisplayNameFor(model => model.ConstructorId)</td>
            <td>@Html.DropDownListFor(model => model.ConstructorId, Model.Companies, new { @class = "dropdown-item", @style = "border: 1;" })</td>
        </tr>
        <tr>
            <td>@Html.DisplayNameFor(model => model.NonDependentControllerId)</td>
            <td>@Html.DropDownListFor(model => model.NonDependentControllerId, Model.Companies, new { @class = "dropdown-item", @style = "border: 1;" })</td>
        </tr>
        <tr>
            <td>@Html.DisplayNameFor(model => model.ContractorId)</td>
            <td>@Html.DropDownListFor(model => model.ContractorId, Model.Companies, new { @class = "dropdown-item", @style = "border: 1;" })</td>
        </tr>
    </table>
    <div class="row h-100 justify-content-center align-items-center">
        <button class="btn btn-primary btn-outline-success align-content-center" type="submit">Добавить</button>
    </div>
}