        </tr>
        <tr>
            <td>@Html.DisplayNameFor(model => model.OwnerId)</td>
            <td>@Html.DropDownListFor(model => model.OwnerId, Model.Companies, new { @class = "dropdown-item", @style = "border: 1;" })</td>
        </tr>
        <tr>
            <td>@Html.DisplayNameFor(model => model.InvestorId)</td>
            <td>@Html.DropDownListFor(model => model.InvestorId, Model.Companies, new { @class = "dropdown-item", @style = "border: 1;" })</td>
        </tr>
        <tr>
            <td>@Html.DisplayNameFor(model => model.InvestorDepartmentId)</td>
            <td>@Html.DropDownListFor(model => model.InvestorDepartmentId, Model.Companies, new { @class = "dropdown-item", @style = "border: 1;" })</td>
        </tr>
        <tr>
            <td>@Html.DisplayNameFor(model => model.GeneralContractorId)</td>
            <td>@Html.DropDownListFor(model => model.GeneralContractorId, Model.Companies, new { @class = "dropdown-item", @style = "border: 1;" })</td>
        </tr>
